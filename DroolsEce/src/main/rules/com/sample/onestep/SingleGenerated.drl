package com.sample.onestep

import com.sample.*;

rule "Fulfilment 80442"
when
	SiAlza
	Fluent(name=="Pippo", $val_Pippo:value);
Fluent(name=="Pluto", $val_Pluto:value);
Fluent(name=="Topolino", $val_Topolino:value);
eval($val_Pippo+$val_Pluto<$val_Topolino+5.0);
	Fluent(name=="Paperino", $val_Paperino:value);
Fluent(name=="Pippo", $val_Pippo:value);
eval($val_Paperino-8.0>$val_Pippo);
then
	System.out.println("<code if fulfilled>");
	System.out.println("FULFILLED :)");
end

rule "Violation 85910"
when
	SiAlza
	not Fluent(name=="Pippo", $val_Pippo:value);
Fluent(name=="Pluto", $val_Pluto:value);
Fluent(name=="Topolino", $val_Topolino:value);
eval($val_Pippo+$val_Pluto<$val_Topolino+5.0);
	Fluent(name=="Paperino", $val_Paperino:value);
Fluent(name=="Pippo", $val_Pippo:value);
eval($val_Paperino-8.0>$val_Pippo);
then
	System.out.println("<code if violated>");
	System.out.println("VIOLATED :(");
end


